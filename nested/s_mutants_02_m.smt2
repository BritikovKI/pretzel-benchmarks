(set-logic HORN)
(declare-fun itp1 (Int Int Int Int) Bool)
(declare-fun itp2 (Int Int Int Int) Bool)
(declare-fun fail () Bool)
(assert (forall ((x1 Int) (x3 Int) (x5 Int) (x7 Int))
  (=> (and (= x1 0) (= x3 0) (= x5 0) (= x7 0)) (itp1 x1 x3 x5 x7))))
(assert (forall ((x7 Int) (x3 Int) (x1 Int) (x5 Int))
  (=> (itp1 x1 x3 x5 x7) (itp2 x7 x3 x1 x5))))
(assert (forall ((x1 Int)
         (x3 Int)
         (x5 Int)
         (x7 Int)
         (x2 Int)
         (x4 Int)
         (x6 Int)
         (x8 Int))
  (=> (and (itp2 x1 x3 x5 x7)
           (= x2 (+ x1 1))
           (= x4 (+ x3 x2))
           (= x6 (+ x5 x4))
           (= x8 (+ x7 x6)))
      (itp2 x2 x4 x6 x8))))
(assert (forall ((x3 Int) (x7 Int) (x5 Int) (x1 Int))
  (=> (itp2 x1 x3 x5 x7) (itp1 x3 x7 x5 x1))))
(assert (forall ((x1 Int) (x3 Int) (x5 Int) (x7 Int))
  (=> (and (itp1 x1 x3 x5 x7) (not (>= x7 0))) fail)))
(assert (=> fail false))
(check-sat)
